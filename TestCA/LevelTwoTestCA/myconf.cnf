dir				= .
oid_section		= new_oids

[ new_oids ]
testoid1=1.2.3.4.5.10.11
testoid2=${testoid1}.5.6

[ ca ]
default_ca				= CA_default		# The default ca section

[ CA_default ]
certs					= $dir/Certs
crl_dir					= $dir/Crl
database				= $dir/index.txt
new_certs_dir			= $dir/Certs

serial					= $dir/serial 		# The current serial number

x509_extensions			= usr_ext		# The extentions to add to the cert
default_days			= 3650			# how long to certify for
default_crl_days		= 30			# how long before next CRL
default_md				= sha256			# which md to use.
certificate				= LevelTwoTestCACert.pem
private_key				= LevelTwoTestCAKey.pem
policy					= usr_policy


[ usr_policy ]
stateOrProvinceName		=	supplied
countryName				=	supplied
commonName				=	supplied
emailAddress			=	supplied
organizationName		=	supplied


[ req ]
default_bits			=	2048
x509_extensions			=	usr_ext
distinguished_name		=	req_distinguished_name
policy					= 	usr_policy


[ usr_ext ]
basicConstraints		= CA:false
#nsCertType				= server 
keyUsage 				= nonRepudiation, digitalSignature, keyEncipherment
subjectAltName 			= @alt_names
authorityInfoAccess		= @aia_sect 
subjectKeyIdentifier	= hash
authorityKeyIdentifier	= keyid,issuer:always

certificatePolicies=ia5org,1.5.7.4,1.5.9.7.8



[ req_distinguished_name ]
stateOrProvinceName		=	CA
countryName				=	IN
emailAddress			=	imca@123.com
organizationalUnitName	=	CITRIX-INDIA HULSUR MGROAD PRESTIGE DYNASTY 
commonName				=  	THIS IS	A TEST CERTIFICATE FOR TESTING BIG CERTIFICATES 
localityName			=	Near French Loaf

[alt_names]
DNS.1=altname-2-1.myca.com 
DNS.2=altname-2-2.myca.com 
DNS.3=altname-2-3.myca.com 
DNS.4=altname-2-4.myca.com 
email.1=mail-2-1@myca.com
email.2=mail-2-2@myca.com


[aia_sect]
caIssuers;URI=http://server.whatever.org/cert-path
